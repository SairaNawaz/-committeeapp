<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <stroke
                android:width="@dimen/_1sdp"
                android:color="@color/yellow" />
        </shape>
    </item>
    <item
        android:gravity="center_vertical|right"
        android:right="@dimen/_1sdp">
        <layer-list>
            <item
                android:width="@dimen/_10sdp"
                android:height="@dimen/_10sdp"
                android:bottom="@dimen/_10sdp"
                android:gravity="center|end"
                android:right="@dimen/_5sdp"
                android:top="@dimen/_10sdp">
                <rotate
                    android:fromDegrees="45"
                    android:toDegrees="45">
                    <shape android:shape="rectangle">
                        <solid android:color="#666666" />
                        <stroke
                            android:width="@dimen/_1sdp"
                            android:color="#aaaaaa" />
                    </shape>
                </rotate>
            </item>
            <item
                android:width="@dimen/_30sdp"
                android:height="@dimen/_10sdp"
                android:bottom="@dimen/_15sdp"
                android:gravity="center"
                android:top="@dimen/_2sdp">
                <shape android:shape="rectangle">
                    <solid android:color="@color/white" />
                    <!-- <stroke android:color="@color/yellow" android:width="@dimen/_2sdp"/>-->
                </shape>
            </item>
        </layer-list>
    </item>
</layer-list>